{
  "name": "@web3-storage/api",
  "version": "5.2.0",
  "private": true,
  "type": "module",
  "license": "(Apache-2.0 OR MIT)",
  "main": "./dist/index.mjs",
  "scripts": {
    "lt": "npm-run-all -p lt:*",
    "lt:cluster": "npx localtunnel --port 9094 --subdomain \"$(whoami)-cluster-api-web3-storage\"",
    "lt:postgres": "npx localtunnel --port 3000 --subdomain \"$(whoami)-postgres-api-web3-storage\"",
    "start": "npm-run-all -p cluster:start pg:start -s miniflare",
    "stop": "npm-run-all -p cluster:stop pg:stop",
    "dev": "wrangler dev --env $(whoami)",
    "publish": "wrangler publish --env $(whoami)",
    "build": "node scripts/cli.js build",
    "test": "npm-run-all -p -r mock:backup test:worker -s test:size",
    "test:worker": "mocha --require test/hooks.js --experimental-vm-modules --timeout 5000",
    "test:size": "bundlesize",
    "mock:backup": "smoke -p 9096 test/mocks/backup",
    "cluster:start": "web3-storage-tools cluster --start",
    "cluster:stop": "web3-storage-tools cluster --stop",
    "cluster:reset": "web3-storage-tools cluster --clean",
    "pg:start": "cd ../db && npm start",
    "pg:stop": "cd ../db && npm run stop",
    "miniflare": "miniflare --env .env.local --watch"
  },
  "devDependencies": {
    "@sentry/cli": "^1.72.1",
    "@types/mocha": "^9.0.0",
    "@web-std/fetch": "^3.0.2",
    "@web-std/form-data": "^3.0.0",
    "@web3-storage/tools": "^1.0.0",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "delay": "^5.0.0",
    "dotenv": "^10.0.0",
    "esbuild": "^0.14.14",
    "git-rev-sync": "^3.0.1",
    "miniflare": "^2.2.0",
    "mocha": "^9.2.0",
    "npm-run-all": "^4.1.5",
    "p-defer": "^4.0.0",
    "process": "^0.11.10",
    "sade": "^1.8.1",
    "smoke": "^3.1.1",
    "stream": "^0.0.2",
    "stream-browserify": "^3.0.0",
    "toucan-js": "^2.4.1",
    "websocket": "^1.0.34"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.28.0",
    "@cfworker/json-schema": "^1.8.3",
    "@google-cloud/precise-date": "^2.0.4",
    "@ipld/car": "^3.1.4",
    "@ipld/dag-cbor": "^6.0.3",
    "@ipld/dag-pb": "^2.0.2",
    "@magic-ext/oauth": "^0.8.0",
    "@magic-sdk/admin": "^1.3.0",
    "@nftstorage/ipfs-cluster": "^3.5.0",
    "@web3-storage/db": "^4.0.0",
    "@web3-storage/multipart-parser": "^1.0.0",
    "cborg": "^1.6.0",
    "ipfs-car": "^0.6.1",
    "itty-router": "^2.4.8",
    "multiformats": "^9.5.8",
    "protobufjs": "^6.11.2",
    "tweetnacl": "^1.0.3",
    "uint8arrays": "^3.0.0"
  },
  "bundlesize": [
    {
      "path": "./dist/index.mjs",
      "maxSize": "1 MB"
    }
  ]
}
